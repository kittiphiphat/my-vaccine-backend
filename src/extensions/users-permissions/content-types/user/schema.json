{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "vaccine_bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vaccine-booking.vaccine-booking",
      "mappedBy": "users_permissions_user"
    },
    "admin_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::admin-log.admin-log",
      "mappedBy": "user"
    },
    "patient_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::patient-log.patient-log",
      "mappedBy": "user"
    },
    "patient": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::patient.patient",
      "mappedBy": "user"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "confirmed",
        "cancelled"
      ],
      "default": "confirmed"
    }
  }
}
